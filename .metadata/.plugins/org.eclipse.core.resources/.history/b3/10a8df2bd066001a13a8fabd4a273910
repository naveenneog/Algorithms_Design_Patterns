package basics;

import java.util.ArrayList;
import java.util.List;

public class BasicArrayOperations {

	static int[] array= new int[]{2,1,3,4,8,5,6};
	static List arraylist = new ArrayList<>();
	static int[] emptyArray = {};
	
	public static void main(String[] args) {
		array = emptyArray;
		arraylist.add(emptyArray[0]);
		List outputArray = new ArrayList();

        for(int el : array){
            outputArray.add(el);
        }
       System.out.println(outputArray.toArray());
	}
	
}
